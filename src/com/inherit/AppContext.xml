<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="veh" class="com.inherit.Vehicle">
		<property name="id" value="1023">
		</property>
		<property name="name" value="Swift"></property>
		<property name="model" value="Desire-ZDi"></property>
	</bean>
	<bean id="vehc1" parent="veh">
		<property name="id" value="1032">
		</property>
		<property name="name" value="Honda"></property>
		<property name="model" value="Civic"></property>
	</bean>
	<bean id="vehc2" class="com.inherit.Vehicle">
		<property name="id" value="1015">
		</property>

	</bean>
	

</beans>